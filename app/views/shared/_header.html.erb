
<header class="site-navbar site-navbar-target" role="banner">
  <div class="container">
    <div class="row align-items-center position-relative">
      <div class="col-3">
        <div class="site-logo">
          <a href="#"><strong>CarRental</strong></a>
        </div>
      </div>
      <div class="col-9  text-right">
        <span class="d-inline-block d-lg-none"><a href="#" class=" site-menu-toggle js-menu-toggle py-5 "><span class="icon-menu h3 text-black"></span></a></span>
        <nav class="site-navigation text-right ml-auto d-none d-lg-block" role="navigation">
          <ul class="site-menu main-menu js-clone-nav ml-auto ">
            <li class="active"><%= link_to  "Home", root_path, class: "nav-link"   %></li>
            <li><%= link_to  "Listing", website_listing_path, class: "nav-link" %></li>
            <li><%= link_to  "Testimonial", website_testimonial_path, class: "nav-link"  %></li>
            <li><%= link_to  "Blog", website_blog_path, class: "nav-link"  %></li>
            <li><%= link_to  "About", website_about_path, class: "nav-link"  %></li>
            <li><%= link_to  "Contact", website_contact_path, class: "nav-link"  %></li>
            <% if user_signed_in? %>
              <li><%= button_to "Sign Out", destroy_user_session_path, class: "nav-link btn btn-outline-danger" ,   method: :delete %></li>
            <% else %>
              <li><%= link_to "Member Ship".html_safe, new_user_session_path, class: " btn btn-success text-white py-2 p-2" %></li>
            <% end %>
          </ul>
        </nav>
      </div>
    </div>
  </div>
</header>